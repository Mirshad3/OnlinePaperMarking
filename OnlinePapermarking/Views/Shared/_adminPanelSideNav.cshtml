                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <!-- End Navbar -->

            <div class="table-responsive" style="display:none">
                <table class="table" id="errorTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>ExamNo</th>
                            <th>Student Name</th>
                            <th>ExamType</th>
                            <th>Subject</th>
