                        <a href="javascript:void(0)" id="englishTab" class="">English</a>
                        <a href="javascript:void(0)" id="tamilTab" class="">தமிழ்</a>
                    </div>

                    <!-- Examination Type -->
                    <div class="home-Examination-type appear-animation" data-appear-animation="fadeInUpShorter" data-appear-animation-delay="400">
                        <div class="row">
                            <div class="col-sm-6"><a href="javascript:void(0)" id="olTab" class="active"><p></p><span>අ.පො.ස. සාමාන්‍ය පෙළ</span></a></div>
                            <div class="col-sm-6"><a href="javascript:void(0)" id="alTab" class=""><p></p><span>අ.පො.ස. උසස් පෙළ</span></a></div>
                        </div>
                    </div>

                    <!-- Past papers -->
                    <div class="appear-animation" data-appear-animation="fadeInUpShorter" data-appear-animation-delay="300">
                        <!-- Sinhala Past papers -->
                        <div class="download-subjects-list Subjects-languageSinhala">
                            <div id="sinhalaPapers" class="row">

                            </div>
                        </div>

                        <!-- English Past papers -->
                        <div class="download-subjects-list Subjects-languageEnglish">
                            <div id="englishPapers" class="row">
                            </div>
                        </div>

                        <!-- Tamil Past papers -->
                        <div class="download-subjects-list Subjects-languageTamil">
                            <div id="tamilPapers" class="row">
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Past papers List -->
            <div class="container pb-5 pastpaper-container" id="papersList">
                <!-- Past paper Item -->
                @*<div class="pastpaper-list">
                    <p>General Certificate of Education (Advanced Level) Examination - 2019 August (Old Syllabus) - Higher Mathematics </p>
                    <a href="#" class="btn btn-secondary mb-2">Preview</a>
