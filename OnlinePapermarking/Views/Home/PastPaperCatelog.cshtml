                                                <li class="dropdown dropdown-mega dropdown-mega-signin signin signin-custom ml-lg-3 mobile-hide" id="headerAccount">
                                                    <div class="header-button d-none d-sm-flex border-right-0 border-left-0">
                                                        <a class="btn btn-rounded btn-primary main-login-btn" href="javascript:void(0)">LOGIN</a>
                                                    </div>
                                                    <ul class="dropdown-menu">
                                                        <li>
                                                            <div class="dropdown-mega-content">
                                                                <div class="row">
                                                                    <div class="col">
                                                                        <div class="alert" id="invalidAlert">
                                                                            Invalid Username or Password
                                                                        </div>
                                                                        <!-- SignIn - START -->
                                                                        <div class="signin-form">
                                                                            <div class="wr-registerForm">

                                                                                <!-- Login partial layout - START -->
                                                                                <span class="top-sub-title text-4 font-weight-bold">Sign In</span>
                                                                                <h2 class="font-weight-bold text-8 mb-5">Welcome Back</h2>
                                                                                <form id="shopLoginRegister">
                                                                                    <fieldset id="SubmitForm">

                                                                                        <div class="form-row">
                                                                                            <div class="form-group col-lg-12">
                                                                                                <label class="text-color-dark" for="">Username</label>
                                                                                                <input type="text" value="" class="form-control bg-light-5 border-0 rounded" name="Email" id="" required>
                                                                                            </div>
                                                                                            <div class="form-group col-lg-12">
                                                                                                <label class="text-color-dark" for="">Password</label>
                                                                                                <input type="password" value="" class="form-control bg-light-5 border-0 rounded" name="Password" id="txtUserPassword" required>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-row mb-3">
                                                                                            <div class="form-group col">
                                                                                                <div class="form-check checkbox-custom checkbox-default">
                                                                                                    <input class="form-check-input" name="IsRemember" type="checkbox" value="true" id="signInRemember">
                                                                                                    <label class="form-check-label" for="signInRemember">
                                                                                                        Remember me
                                                                                                    </label>
                                                                                                </div>
                                                                                            </div>
                                                                                            <div class="form-group col text-right">
                                                                                                <a href="#" id="headerRecover" class="forgot-pw text-color-dark d-block">Forgot password?</a>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-row mb-5">
                                                                                            <div class="col-12 text-left">
                                                                                                <a class="btn btn-primary btn-4 btn-fs-4 mb-2 btn-rounded btn-block" style="color:whitesmoke" id="loginBtn">Sign In</a>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-row">
                                                                                            <p class="col text-center p-0">
                                                                                                Don't have have an account? <a href="/Home/Register" class="text-primary">SignUp</a>
                                                                                            </p>
                                                                                        </div>
                                                                                    </fieldset>
                                                                                </form>
                                                                                <!-- Login partial layout - END -->

                                                                            </div>
                                                                        </div><!-- SignIn - End -->
                                                                        <!-- Password recovery - START -->
                                                                        <div class="recover-form wr-registerForm">
                                                                            <span class="top-sub-title text-4 font-weight-bold">Members</span>
                                                                            <h2 class="font-weight-bold text-8 mb-5">Reset my Password</h2>

                                                                            <ul class="nav nav-tabs nav-tabs-default registerForm-as" id="tabDefault" role="tablist">
                                                                                <li class="nav-item">
                                                                                    <a class="nav-link active show" id="default-portfolio-tab" data-toggle="tab" href="#default-portfolio" role="tab" aria-controls="default-portfolio" aria-expanded="true">
                                                                                        <h4>Email</h4>
                                                                                    </a>
                                                                                </li>
                                                                                <li class="nav-item">
                                                                                    <a class="nav-link" id="default-process-tab" data-toggle="tab" href="#default-process" role="tab" aria-controls="default-process">
                                                                                        <h4>Mobile</h4>
                                                                                    </a>
                                                                                </li>
                                                                            </ul>
                                                                            <div class="tab-content" id="tabDefaultContent">
                                                                                <div class="tab-pane fade pt-4 show active" id="default-portfolio" role="tabpanel" aria-labelledby="default-portfolio-tab">
                                                                                    <form action="/" id="frmResetPassword" method="post">
                                                                                        <div class="form-row mb-4">
                                                                                            <div class="form-group col mb-2">
                                                                                                <input type="email" value="" maxlength="100" class="form-control bg-light-5 border-0 rounded" name="email" id="resetPasswordEmail" placeholder="E-mail" required>
                                                                                            </div>
                                                                                        </div>
                                                                                    </form>
                                                                                </div>
                                                                                <div class="tab-pane fade pt-4" id="default-process" role="tabpanel" aria-labelledby="default-process-tab">
                                                                                    <form action="/" id="frmResetPassword" method="post">
                                                                                        <div class="form-row mb-4">
                                                                                            <div class="form-group col mb-2">
                                                                                                <input type="number" value="" maxlength="100" class="form-control bg-light-5 border-0 rounded" id="" placeholder="Mobile Number" required>
                                                                                            </div>
                                                                                        </div>
                                                                                    </form>
                                                                                </div>
                                                                                <div class="row align-items-center">
                                                                                    <div class="col">
                                                                                        <a href="#" id="headerRecoverCancel" class="text-color-primary pl-0">Have an account?</a>
                                                                                    </div>
                                                                                    <div class="col text-right">
                                                                                        <button type="button" class="btn btn-primary btn-4 btn-fs-4 mb-2 btn-rounded btn-block" id="resetPasswordBtn1">Submit</button>
                                                                                    </div>
                                                                                </div>
                                                                            </div>


                                                                        </div><!-- Password recovery - END -->
                                                                        <!-- Verify Code Email - START -->
                                                                        <div class="email-reset-code-form reset-code-new" style="display:none;">
                                                                            <span class="top-sub-title text-4 font-weight-bold">Verify Email</span>
